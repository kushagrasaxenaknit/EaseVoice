<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="android:textColor">#000000</item>
        <item name="android:textStyle">bold</item>
        <item name="colorPrimary">#21610B</item>
        <item name="colorPrimaryDark">#04B404</item>
        <item name="colorAccent">@android:color/holo_green_light</item>
    </style>
    <style name="MyTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="MyTheme1" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:textColor">#660099</item>
        <item name="android:textStyle">bold</item>
        <item name="colorPrimary">@color/colorAccent</item>
        <item name="colorPrimaryDark">@color/accent_material_light</item>
        <item name="colorAccent">@android:color/holo_purple</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.CustomTheme">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
        <item name="colorPrimaryDark">#04B404</item>
        <item name="colorAccent">@android:color/holo_green_light</item>
        <item name="colorPrimary">#21610B</item>
    </style>
    <style name="AppTheme.CustomTheme1">
        <item name="android:windowCloseOnTouchOutside">true</item>
        </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>